select id,Status, Dispute_Reference__c ,Self_Resolution_Start_Date__c,(select id from Dispute_Items__r  )  from Case where RecordType.name = 'Dispute EW Insured' And Status Not In ('Proposal submitted – awaiting agent/landlord response','Rejected','No response from agent/landlord','Case closed – no dispute','New','Proposal submitted- tenant','Consent to resolution not given','Case closed – resolved without adjudication','Clarification needed')
